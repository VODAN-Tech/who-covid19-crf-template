          a vodan:Hypertension ;
          vodan:has_value vodan_inst:C49487 ;  # NO
        ] ;
        {%- elif crf.comorbHypertension == crf.comorbHypertension_unknown %}
        obo:BFO_0000051 [
          a vodan:Hypertension ;
