          vodan:has_literal_value "{{ crf.ageNumber }}"^^xsd:integer;
        ] ;
        {%- elif crf.ageUnits == crf.ageUnits_Months %}
        obo:BFO_0000051 [
          a vodan:Age_months ;
          vodan:has_literal_value "{{ crf.ageNumber }}"^^xsd:integer;
        ] ;
        {%- endif %}
        {%- endif %}
        {#- HEALTHCARE WORKER #}
        {%- if crf.healthCareWorker == crf.healthCareWorker_yes %}
        obo:BFO_0000051 [
          a vodan:Healtcare_worker ;
          vodan:has_value vodan_inst:C49488 ;  # YES
        ] ;
        {%- elif crf.healthCareWorker == crf.healthCareWorker_no %}
        obo:BFO_0000051 [
          a vodan:Healtcare_worker ;
          vodan:has_value vodan_inst:C49487 ;  # NO
        ] ;
        {%- elif crf.healthCareWorker == crf.healthCareWorker_unknown %}
        obo:BFO_0000051 [
          a vodan:Healtcare_worker ;
          vodan:has_value vodan_inst:C17998 ;  # UNKNOWN
        ] ;
        {%- endif %}
        {#- LABORATORY WORKER #}
        {%- if crf.laboratoryWorker == crf.laboratoryWorker_yes %}
        obo:BFO_0000051 [
          a vodan:Laboratory_Worker ;
          vodan:has_value vodan_inst:C49488 ;  # YES
        ] ;
        {%- elif crf.laboratoryWorker == crf.laboratoryWorker_no %}
        obo:BFO_0000051 [
          a vodan:Laboratory_Worker ;
          vodan:has_value vodan_inst:C49487 ;  # NO
        ] ;
        {%- elif crf.laboratoryWorker == crf.laboratoryWorker_unknown %}
        obo:BFO_0000051 [
          a vodan:Laboratory_Worker ;
          vodan:has_value vodan_inst:C17998 ;  # UNKNOWN
