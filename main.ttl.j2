        ] ;
        {%- elif crf.pregnant == crf.pregnant_unknown %}
        obo:BFO_0000051 [
          a vodan:Pregnant ;
          vodan:has_value vodan_inst:C17998 ;  # UNKNOWN
        ] ;
        {%- elif crf.pregnant == crf.pregnant_na %}
        obo:BFO_0000051 [
          a vodan:Pregnant ;
          vodan:has_value vodan_inst:C18902 ;
        ] ;
        {%- endif %}
