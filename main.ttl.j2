        {%- elif crf.admissionSternalRefill == crf.admissionSternalRefill_unknown %}
        obo:BFO_0000051 [
          a vodan:Capillary_refill_admission ;
          vodan:has_value vodan_inst:C17998 ;  # UNKNOWN
        ] ;
        {%- endif %}
        {#- OXYGEN SATURATION #}
        obo:BFO_0000051 [
          a vodan:Oxygen_saturation_admission ;
          vodan:has_literal_value "{{ crf.admissionOxygenSatValue }}"^^xsd:integer ;
        ] ;
        {%- if crf.admissionOxygenSatCond == crf.admissionOxygenSatCond_Room %}
        obo:BFO_0000051 [
          a vodan:Oxygen_saturation_expl_admission ;
          vodan:has_value vodan:oxygen_therapy ;
        ] ;
        {%- elif crf.admissionOxygenSatCond == crf.admissionOxygenSatCond_Therapy %}
        obo:BFO_0000051 [
          a vodan:Oxygen_saturation_expl_admission ;
          vodan:has_value vodan:room_air ;
        ] ;
        {%- elif crf.admissionOxygenSatCond == crf.admissionOxygenSatCond_Unknown %}
        obo:BFO_0000051 [
          a vodan:Oxygen_saturation_expl_admission ;
          vodan:has_value vodan_inst:C17998 ;  # UNKNOWN
        ] ;
        {%- endif %}
        {#- AVPU #}
        {%- if crf.admissionAVPU == crf.admissionAVPU_A %}
        obo:BFO_0000051 [
          a vodan:AVPU_scale_admission ;
          vodan:has_value vodan_avpu:A ;
        ] ;
        {%- elif crf.admissionAVPU == crf.admissionAVPU_V %}
        obo:BFO_0000051 [
          a vodan:AVPU_scale_admission ;
          vodan:has_value vodan_avpu:V ;
        ] ;
        {%- elif crf.admissionAVPU == crf.admissionAVPU_P %}
        obo:BFO_0000051 [
          a vodan:AVPU_scale_admission ;
          vodan:has_value vodan_avpu:P ;
        ] ;
        {%- elif crf.admissionAVPU == crf.admissionAVPU_U %}
        obo:BFO_0000051 [
          a vodan:AVPU_scale_admission ;
          vodan:has_value vodan_avpu:U ;
        ] ;
        {%- endif %}
        {#- GLASGOW COMA SCORE #}
        obo:BFO_0000051 [
          a vodan:GCS_admission ;
          vodan:has_literal_value "{{ crf.admissionGCS }}"^^xsd:integer ;
        ] ;
        {#- Malnutrition #}
        {%- if crf.admissionMalnutrition == crf.admissionMalnutrition_yes %}
        obo:BFO_0000051 [
          a vodan:Malnutrition ;
          vodan:has_value vodan_inst:C49488 ;  # YES
        ] ;
        {%- elif crf.admissionMalnutrition == crf.admissionMalnutrition_no %}
        obo:BFO_0000051 [
          a vodan:Malnutrition ;
          vodan:has_value vodan_inst:C49487 ;  # NO
        ] ;
        {%- elif crf.admissionMalnutrition == crf.admissionMalnutrition_unknown %}
        obo:BFO_0000051 [
          a vodan:Malnutrition ;
          vodan:has_value vodan_inst:C17998 ;  # UNKNOWN
        ] ;
        {%- endif %}
        {#- ARM CIRCUMFERENCE #}
        obo:BFO_0000051 [
          a vodan:Arm ;
          vodan:has_literal_value "{{ crf.admissionCircumeference }}"^^xsd:integer ;
        ] ;
        {#- HEIGHT #}
        obo:BFO_0000051 [
          a vodan:Height ;
          vodan:has_literal_value "{{ crf.admissionHeight }}"^^xsd:integer ;
        ] ;
        {#- WEIGHT #}
        obo:BFO_0000051 [
          a vodan:Weight ;
          vodan:has_literal_value "{{ crf.admissionWeight }}"^^xsd:integer ;
        ] ;
      ] ;
      {%- endif %}
      {#- CO-MORBIDITIES #}
      {%- if crf.comorbidities == crf.comorbidities_open %}
      # ADMISSION: CO-MORBIDITIES
      obo:BFO_0000051 [
        a vodan:Co-morbidities ;
