        ] ;
        {%- endif %}
        {%- if crf.comorbChronicPulmonary == crf.comorbChronicPulmonary_yes %}
        obo:BFO_0000051 [
          a vodan:Pulmonary_disease ;
          vodan:has_value vodan_inst:C49488 ;  # YES
        ] ;
        {%- elif crf.comorbChronicPulmonary == crf.comorbChronicPulmonary_no %}
        obo:BFO_0000051 [
          a vodan:Pulmonary_disease ;
          vodan:has_value vodan_inst:C49487 ;  # NO
        ] ;
        {%- elif crf.comorbChronicPulmonary == crf.comorbChronicPulmonary_unknown %}
        obo:BFO_0000051 [
          a vodan:Pulmonary_disease ;
