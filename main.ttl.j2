        {%- if crf.sexAtBirth == crf.sexAtBirth_Male %}
        obo:BFO_0000051 [
          a vodan:Sex ;
          vodan:has_value vodan_inst:C46109;
        ] ;
        {%- elif crf.sexAtBirth == crf.sexAtBirth_Female %}
        obo:BFO_0000051 [
          a vodan:Sex ;
          vodan:has_value vodan_inst:C46110;
        ] ;
        {%- elif crf.sexAtBirth == crf.sexAtBirth_NotSpecified %}
        obo:BFO_0000051 [
          a vodan:Sex ;
          vodan:has_value vodan_inst:C38046;
        ] ;
        {%- endif %}
        {#- DATE OF BIRTH / AGE #}
        {%- if crf.dateOfBirth == crf.dateOfBirthKnown_yes %}
        obo:BFO_0000051 [
          a vodan:Birth ;
          vodan:has_literal_value "{{ crf.dateOfBirthDate }}"^^xsd:date;
        ] ;
        {%- elif crf.dateOfBirth == crf.dateOfBirthKnown_no %}
        {%- if crf.ageUnits == crf.ageUnits_Years %}
        obo:BFO_0000051 [
