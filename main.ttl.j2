{%- import "_mapping.j2" as crf with context -%}
{%- set repliesMap = ctx.questionnaireRepliesMap -%}
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix vodan: <http://purl.org/vodan/whocovid19crfsemdatamodel/> .
@prefix vodan_inst: <http://purl.org/vodan/whocovid19crfsemdatamodel/instances/> .
@prefix vodan_avpu: <http://purl.org/vodan/whocovid19crfsemdatamodel/instances/avpu-list/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
# Using VODAN ontology 1.1.4
# See: https://github.com/FAIRDataTeam/WHO-COVID-CRF

<{{ ctx.config.clientUrl }}/crfs/detail/{{ ctx.questionnaireUuid }}> a vodan:who-covid-19-rapid-crf ;
    {#- MODULE 1 #}
    {%- if crf.enrolmentDate|length > 0 %}
    # MODULE 1 (ADMISSION)
    obo:BFO_0000051 [
      a vodan:Module_1 ;
      obo:BFO_0000051 [
        a vodan:Facility_name ;
        vodan:has_literal_value "{{ crf.siteName }}";
      ] ;
      obo:BFO_0000051 [
        a vodan:Country ;
        vodan:has_value <{{crf.country__uri}}>;
      ] ;
      obo:BFO_0000051 [
        a vodan:Enrolment ;
        vodan:has_literal_value "{{ crf.enrolmentDate }}"^^xsd:date;
      ] ;
      {#- INCLUSION CRITERIA #}
      {%- if crf.inclusionCriteria == crf.inclusionCriteria_open %}
      # ADMISSION: INCLUSION CRITERIA
      obo:BFO_0000051 [
        a vodan:Inclusion ;
        {%- if crf.pathogenPHI == crf.pathogenPHI_yes %}
        obo:BFO_0000051 [
          a vodan:Inclusion_pathogen ;
          vodan:has_literal_value "true"^^xsd:boolean;
        ] ;
        {%- if crf.phiFeverHistory == crf.phiFeverHistory_yes %}
        obo:BFO_0000051 [
          a vodan:Inclusion_feverishness ;
          vodan:has_literal_value "true"^^xsd:boolean;
        ] ;
        {%- elif crf.phiFeverHistory == crf.phiFeverHistory_no %}
        obo:BFO_0000051 [
          a vodan:Inclusion_feverishness ;
          vodan:has_literal_value "false"^^xsd:boolean;
        ] ;
        {%- endif %}
        {%- if crf.phiCough == crf.phiCough_yes %}
        obo:BFO_0000051 [
          a vodan:Inclusion_cough ;
          vodan:has_literal_value "true"^^xsd:boolean;
        ] ;
        {%- elif crf.phiCough == crf.phiCough_no %}
        obo:BFO_0000051 [
          a vodan:Inclusion_cough ;
          vodan:has_literal_value "false"^^xsd:boolean;
        ] ;
        {%- endif %}
        {%- if crf.phiDyspnoea == crf.phiDyspnoea_yes %}
        obo:BFO_0000051 [
          a vodan:Inclusion_dyspnoea ;
          vodan:has_literal_value "true"^^xsd:boolean;
        ] ;
        {%- elif crf.phiDyspnoea == crf.phiDyspnoea_no %}
        obo:BFO_0000051 [
          a vodan:Inclusion_dyspnoea ;
          vodan:has_literal_value "false"^^xsd:boolean;
        ] ;
        {%- endif %}
        {%- if crf.phiARI == crf.phiARI_yes %}
        obo:BFO_0000051 [
          a vodan:Inclusion_ARI ;
          vodan:has_literal_value "true"^^xsd:boolean;
        ] ;
        {%- elif crf.phiARI == crf.phiARI_no %}
        obo:BFO_0000051 [
          a vodan:Inclusion_ARI ;
          vodan:has_literal_value "false"^^xsd:boolean;
        ] ;
        {%- endif %}
        {%- elif crf.pathogenPHI == crf.pathogenPHI_no %}
        obo:BFO_0000051 [
          a vodan:Inclusion_pathogen ;
          vodan:has_literal_value "false"^^xsd:boolean;
        ] ;
        {%- endif %}
      ] ;
      {%- endif %}
